{
  "name": "md5-js-tools",
  "version": "1.0.2",
  "description": "Generate md5 hashes",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "minify": "webpack",
    "format": "prettier --write \"src/**/*.ts\" \"*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build && webpack --env path=lib",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simetrio/md5-js-tools.git"
  },
  "keywords": [
    "md5"
  ],
  "author": "simetrio",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/simetrio/md5-js-tools/issues"
  },
  "homepage": "https://www.olrix.net/tools/md5-generator",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1"
  },
  "files": [
    "lib/**/*"
  ]
}
